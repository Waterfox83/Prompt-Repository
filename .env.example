# AWS Configuration
S3_BUCKET_NAME=llm-prompt-repository
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here_if_using_sso
